{
  "name": "brain-agriculture-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.9.0",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build:local": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "check:update": "pnpm update --interactive",
    "dev": "vite dev --mode development",
    "format": "prettier --write .",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:c": "vitest --coverage --reporter=verbose",
    "test:ci": "vitest --silent",
    "test:ui": "vitest --coverage --ui",
    "test:w": "vitest watch",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@faker-js/faker": "9.3.0",
    "@floating-ui/core": "1.6.8",
    "@floating-ui/react": "0.27.2",
    "@floating-ui/react-dom": "2.1.2",
    "@hookform/resolvers": "3.9.1",
    "@tanstack/react-query": "5.62.14",
    "@tanstack/react-query-devtools": "5.62.14",
    "@testing-library/dom": "10.4.0",
    "@xstyled/styled-components": "4.0.0",
    "@xstyled/system": "3.8.1",
    "axios": "1.7.9",
    "date-fns": "4.1.0",
    "downshift": "9.0.8",
    "eslint-import-resolver-typescript": "3.7.0",
    "i18next": "24.2.0",
    "i18next-browser-languagedetector": "8.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "5.0.0",
    "react-hook-form": "7.54.2",
    "react-i18next": "15.4.0",
    "react-imask": "7.6.1",
    "react-router": "7.1.1",
    "styled-bettertools": "0.5.1",
    "styled-components": "6.1.13",
    "yup": "1.6.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react-swc": "3.7.2",
    "@vitest/ui": "3.0.0-beta.3",
    "brain-agriculture-backend-types": "workspace:*",
    "jsdom": "25.0.1",
    "typescript": "5.7.2",
    "vite": "6.0.7",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.0-beta.3",
    "vitest-mock-extended": "2.0.2"
  }
}