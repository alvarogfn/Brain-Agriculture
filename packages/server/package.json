{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "cross-env MODE=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env MODE=production nest start",
    "start:dev": "cross-env MODE=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prisma:sync": "npx prisma db push && npx prisma generate",
    "start:prod": "cross-env MODE=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:studio": "npx prisma studio",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate"
  },
  "dependencies": {
    "@nestjs/common": "10.4.15",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.15",
    "@nestjs/platform-express": "10.4.15",
    "@nestjs/swagger": "8.1.0",
    "@prisma/client": "6.1.0",
    "brain-agriculture-backend-types": "workspace:*",
    "cookie-parser": "1.4.7",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "9.3.0",
    "@nestjs/cli": "10.4.9",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.15",
    "@types/cookie-parser": "1.4.8",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.5",
    "@types/supertest": "6.0.2",
    "cross-env": "7.0.3",
    "jest": "29.7.0",
    "jest-mock-extended": "4.0.0-beta1",
    "prisma": "6.1.0",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.2"
  },
  "prisma": {
    "schema": "src/database/schema.prisma"
  }
}
